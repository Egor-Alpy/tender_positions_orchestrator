# API ключи для сервисов
CLASSIFIER_API_KEY=your_classifier_api_key
STANDARDIZER_API_KEY=your_standardizer_api_key
MATCHER_API_KEY=your_matcher_api_key

# Настройки безопасности оркестратора
REQUIRE_API_KEY=false
ORCHESTRATOR_API_KEY=your_orchestrator_api_key

# URLs сервисов (по умолчанию используются имена из docker-compose)
# CLASSIFIER_URL=http://classifier:8000/api/v1/tender/classify-tender
# STANDARDIZER_URL=http://standardizer:8000/api/v1/standardize
# MATCHER_URL=http://matcher:8000/api/v1/match

# Таймаут для HTTP запросов (в секундах)
REQUEST_TIMEOUT=300

# Уровень логирования (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO